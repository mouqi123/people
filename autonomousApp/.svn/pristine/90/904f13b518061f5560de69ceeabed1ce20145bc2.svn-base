<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<title></title>
<#include "/jeecms_sys/head.html"/>
<script type="text/javascript">
	$(function() {
		$("#jvForm").validate();
	});
	$(document).ready(function() {
	});
</script>
<style type="text/css">
.perm-container{}
.perm-layout-1{padding:3px 0;}
.perm-layout-2{padding-left:30px;}
</style>
</head>
<body>
<div class="box-positon">
	<div class="rpos"></div>
		<form class="ropt">
			<input type="submit" value="<@s.m "global.backToList"/>" onclick="this.form.action='v_list.do';" class="return-button"/>
		</form>
		<div class="clear"></div>
</div>
<div class="body-box">
<@p.form id="jvForm" action="o_update.do" labelWidth="20">
<@p.text colspan="1" width="50" label="AppId" name="appId" value=ApplyDO.appId  required="true" class="required"/>
<@p.text colspan="1" width="50" label="AppKey" name="appKey" value=ApplyDO.appKey required="true" class="required" /><@p.tr/>
<@p.text colspan="1" width="50" label="App平台" name="appType" value=ApplyDO.appType   required="true" class="required" />
<@p.text colspan="1" width="50" label="App版本" name="appVersion" value=ApplyDO.appVersion  required="true" class="required" /><@p.tr/>
<@p.text colspan="1" width="50" label="App名称" name="appName" value=ApplyDO.appName  required="true" class="required" />
<@p.text colspan="2" width="50" label="App描述" name="appDesc"  value=ApplyDO.appDesc  required="true" class="required" /><@p.tr/>
<@p.td colspan="2"><@p.hidden name="id" value=ApplyDO.id/><@p.submit code="global.submit"  /> &nbsp; <@p.reset code="global.reset"/></@p.td>
</@p.form>
</div>
</body>
</html>